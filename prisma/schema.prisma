datasource db {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["debian-openssl-1.1.x", "windows"]
}

model GuildSettings {
  guildId String  @id @map("guild_id")
  prefix  String? @db.Text

  @@map("guild_settings")
}
