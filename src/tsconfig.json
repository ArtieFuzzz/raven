{
  "extends": "@artiefuzzz/ts",
  "compilerOptions": {
    "target": "ESNext",
    "outDir": "../build",
    "importsNotUsedAsValues": "error",
    "experimentalDecorators": true,
    "baseUrl": ".",
    "rootDir": ".",
    "paths": {
      "#lib/*": ["./lib/*"],
      "#lib/env": ["./lib/env/parser.ts"],
      "#types": ["./lib/@types/index.ts"],
      "#util": ["./lib/util/index.ts"]
    },
    "importHelpers": true
  },
  "include": ["."],
  "exclude": ["tsconfig.json"]
}
